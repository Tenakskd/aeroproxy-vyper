{
	"compilerOptions": {
		"target": "ESNext",
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"baseUrl": "./",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"paths": {
			"$aero/*": ["*"],
			"$aero_sw/*": ["../../*"],
			"$src/*": ["src/*"],
			"$sandbox/*": ["src/sandboxers/*"],
			"$apis/*": ["src/apis/*"],
			"$cors/*": ["src/cors/*"],
			"$shared/*": ["src/shared/*"],
			"$types/*": ["types/*"]
		},
		"types": ["@types/websql"]
	},
	"include": [
		"rspack.config.ts",
		"src/**/*.ts",
		"types/**/*.d.ts",
		"src/defaultConfig.js",
		"internal/featureFlags.ts",
		"types/contextualIdentities.ts"
	]
}
